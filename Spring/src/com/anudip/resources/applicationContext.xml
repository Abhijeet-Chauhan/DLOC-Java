<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
<bean class="com.anudip.beans.Student" id="std1">
<property name="name" value="Abhijeet"></property>
<property name="rollno" value="17"></property>
<property name="email" value="abhijeet@mail.com"></property>
</bean>

<bean class="com.anudip.beans.Student" id="std2">
<property name="name" value="Abhi"></property>
<property name="rollno" value="101"></property>
<property name="email" value="abhi@mail.com"></property>
</bean>

</beans>